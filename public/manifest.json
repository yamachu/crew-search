{
    "manifest_version": 2,
    "short_name": "Crew Search",
    "name": "Crew Search",
    "version": "0.0.1",
    "browser_action": {
        "default_title": "Ext title",
        "default_popup": "./index.html"
    },
    "background": {
        "scripts": [
            "./background.js"
        ],
        "persistent": false
    },
    "permissions": ["clipboardWrite"],
    "content_security_policy": "script-src 'self' https://apis.google.com https://www.gstatic.com https://*.firebaseio.com https://www.googleapis.com; object-src 'self'; connect-src 'self' wss://*.firebaseio.com https://*.googleapis.com https://*.search.windows.net;",
    "key": ""
}
